#jinja2: lstrip_blocks: "true", trim_blocks: "false"
{% set config = configuration %}
{# Device Configuration #}
{% set config_sections = [
    'system',
    'chassis',
] %}
{% for section in config_sections %}
    {% if 'redundancy' in config.chassis %}
{{ section }} {
    {% filter indent(4, true) %}
    {% include 'ex/' + section + '/main.j2' %}
    {% endfilter %}
}
    {% endif %}
{% endfor %}

